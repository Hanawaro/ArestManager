<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/arestsListFragment">
    <fragment
        android:id="@+id/arestsListFragment"
        android:name="com.speechpeach.arestmanager.ui.fragments.ArestsListFragment"
        android:label="Arests"
        tools:layout="@layout/fragment_arests_list" >
        <action
            android:id="@+id/action_arestsListFragment_to_createUserForArestFragment"
            app:destination="@id/createUserForArestFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_arestsListFragment_to_editArestFragment"
            app:destination="@id/editArestFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/usersListFragment"
        android:name="com.speechpeach.arestmanager.ui.fragments.UsersListFragment"
        android:label="Users"
        tools:layout="@layout/fragment_users_list" >
        <action
            android:id="@+id/action_usersListFragment_to_createUserFragment"
            app:destination="@id/createUserFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/createUserFragment"
        android:name="com.speechpeach.arestmanager.ui.fragments.CreateUserFragment"
        android:label="User editor">
        <argument
            android:name="user"
            app:argType="com.speechpeach.arestmanager.models.User"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/createUserForArestFragment"
        android:name="com.speechpeach.arestmanager.ui.fragments.CreateUserForArestFragment"
        android:label="Users" >
        <action
            android:id="@+id/action_createUserForArestFragment_to_createArestFragment"
            app:destination="@id/createArestFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/createArestFragment"
        android:name="com.speechpeach.arestmanager.ui.fragments.CreateArestFragment"
        android:label="Arest editor"
        tools:layout="@layout/fragment_create_arest">
        <argument
            android:name="user"
            app:argType="com.speechpeach.arestmanager.models.User" />
        <argument
            android:name="isNewUser"
            app:argType="boolean" />
        <action
            android:id="@+id/action_createArestFragment_to_arestsListFragment"
            app:destination="@id/arestsListFragment"
            app:popUpTo="@id/arestsListFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/editArestFragment"
        android:name="com.speechpeach.arestmanager.ui.fragments.EditArestFragment"
        android:label="Arest editor"
        tools:layout="@layout/fragment_edit_arest">
        <action
            android:id="@+id/action_editArestFragment_to_selectUserFragment"
            app:destination="@id/selectUserFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <argument
            android:name="arest"
            app:argType="com.speechpeach.arestmanager.models.Arest" />
    </fragment>
    <fragment
        android:id="@+id/selectUserFragment"
        android:name="com.speechpeach.arestmanager.ui.fragments.SelectUserFragment"
        android:label="User selector"
        tools:layout="@layout/fragment_select_user">
        <action
            android:id="@+id/action_selectUserFragment_to_editArestFragment2"
            app:destination="@id/editArestFragment"
            app:popUpTo="@id/editArestFragment"
            app:popUpToInclusive="true"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <argument
            android:name="arest"
            app:argType="com.speechpeach.arestmanager.models.Arest" />
    </fragment>
</navigation>